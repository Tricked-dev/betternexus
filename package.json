    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@plasmohq/storage": "^0.13.0",
    "framer-motion": "^7.6.19",
    "plasmo": "0.59.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0",
    "@types/chrome": "0.0.203",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "manifest": {
